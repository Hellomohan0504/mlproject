<script>
$(window).scroll(function() {    
    var scroll = $(window).scrollTop();
     //>=, not <=
    if (scroll >= 200) {
        //clearHeader, not clearheader - caps H
        $(".top-bar").addClass("top-bar-fixed");
    }
    else{
      $(".top-bar").removeClass("top-bar-fixed");  
    }
}); //missing )
</script>
