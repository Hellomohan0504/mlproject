<script src="https://hicounselor.com/assets/frontend/js/bootstrap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.5/jquery.validate.min.js"></script>
<script src="https://hicounselor.com/assets/frontend/js/developer.js"></script>
<script src="https://hicounselor.com/assets/frontend/js/interface.js"></script>
